<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 设置domain包下的别名-->
    <typeAliases>
        <package name="com.qq.ssm.domain"/>
    </typeAliases>
    <!-- 设置mapper映射文件地址-->
    <mappers>
        <mapper class="com.qq.ssm.dao"/>
    </mappers>
</configuration>